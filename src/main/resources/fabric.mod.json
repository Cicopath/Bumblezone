{
  "schemaVersion": 1,
  "id": "the_bumblezone",
  "version": "$version",

  "name": "The Bumblezone - Fabric",
  "description": "An awesome dimension full of bees that becomes REALLY angry if you take their honey! Enter the dimension by throwing an enderpearl at Bee Nest and exit it by going above Y = 256 to below Y = 0.",
  "authors": [
    "TelepathicGrunt"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/the-bumblezone-fabric",
    "sources": "https://github.com/TelepathicGrunt/Bumblezone-Fabric",
    "issues": "https://github.com/TelepathicGrunt/Bumblezone-Fabric/issues"
  },

  "license": "MIT",
  "icon": "assets/the_bumblezone/the_bumblezone_logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.telepathicgrunt.the_bumblezone.Bumblezone"
    ],
    "modmenu": [
      "com.telepathicgrunt.the_bumblezone.configs.ModMenuConfig"
    ],
    "client": [
      "com.telepathicgrunt.the_bumblezone.client.BumblezoneClient"
    ],
    "cardinal-components-entity": [
      "com.telepathicgrunt.the_bumblezone.Bumblezone"
    ]
  },
  "mixins": [
    "the_bumblezone.mixins.json"
  ],

  "accessWidener" : "the_bumblezone.accesswidener",

  "depends": {
    "fabricloader": ">=0.12.0",
    "minecraft": ">=1.18.1",
    "fabric": ">=0.43.1",
    "cloth-config2": ">=6.0.42",
    "cardinal-components-base": "*",
    "cardinal-components-entity": "*",
    "feature_nbt_deadlock_be_gone": "*"
  },
  "custom": {
    "cardinal-components": [
      "the_bumblezone:entity_component",
      "the_bumblezone:original_flying_speed"
    ],
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/TelepathicGrunt/Bumblezone-Fabric/latest-released/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
